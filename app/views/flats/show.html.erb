<!DOCTYPE html>
<html>
<head>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDiDbGnl5V8YzlpY4HD7eQ4cYH9iKbkQOE&callback=initMap" async defer></script>
  <title>Flat Show</title>
  <%= stylesheet_link_tag 'show', media: 'all' %>
</head>
<body class="show-page">
  <div class="container">
    <h1 class="show-page__title"><%= @flat.name %></h1>

    <div class="row">
      <div class="col-md-6 show-page__large-picture">
        <% if @flat.large_picture_url.present? %>
          <img src="<%= asset_path(@flat.large_picture_url) %>" class="img-fluid" alt="Large Picture">
        <% else %>
          <p class="show-page__no-picture">No large picture available</p>
        <% end %>
      </div>

      <div class="col-md-6 show-page__small-pictures">
        <div class="row">
          <% if @flat.small_picture_urls.present? %>
            <% @flat.small_picture_urls.each do |url| %>
              <div class="col-md-6 mb-3 show-page__small-picture">
                <img src="<%= asset_path(url) %>" class="img-fluid" alt="Small Picture">
              </div>
            <% end %>
          <% else %>
            <p class="show-page__no-picture">No small pictures available</p>
          <% end %>
        </div>
      </div>
    </div>

    <div class="row mt-4">
      <div class="col-md-6">
        <p class="show-page__address"><strong>Address:</strong> <%= @flat.address %></p>
        <p class="show-page__description"><strong>Description:</strong> <%= @flat.description %></p>

        <div id="map-container">
          <div id="map"></div>
        </div>
      </div>

      <div class="col-md-6 d-flex justify-content-center align-items-center">
        <div class="price-box">
          <div class="price-box__inner">
            <div class="price-box__checkin-checkout">
              <div class="price-box__checkin">
                <div class="price-box__label">Check-in</div>
                <input type="date" class="price-box__input" id="checkin-date">
              </div>
              <div class="price-box__checkout">
                <div class="price-box__label">Checkout</div>
                <input type="date" class="price-box__input" id="checkout-date">
              </div>
            </div>
            <hr>
            <button class="price-box__button">Reserve</button>
            <div class="price-details">
              <div class="price-details__item">
                <div class="price-details__title">Price per night</div>
                <div class="price-details__value" id="price-per-night">£<%= number_to_currency(@flat.price_per_night, unit: '') %></div>
              </div>
              <div class="price-details__item" style="margin-left: auto;">
                <div class="price-details__title">Number of nights</div>
                <input type="number" class="price-details__input" id="num-nights" value="5" style="width: 44px; padding: 2px; margin-left: 10px; border-radius: 5px; border-width: 1.5px;">
              </div>
              <div class="price-details__item">
                <div class="price-details__title">Cleaning fee</div>
                <div class="price-details__value">£99</div>
              </div>
              <div class="price-details__total">
                <div class="price-details__title2">Total</div>
                <div class="price-details__value2" id="total-price">£0</div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>

 <script>
    document.addEventListener('DOMContentLoaded', function() {
      const pricePerNight = document.getElementById('price-per-night');
      const numNightsInput = document.getElementById('num-nights');
      const totalPrice = document.getElementById('total-price');
      const checkinDateInput = document.getElementById('checkin-date');
      const checkoutDateInput = document.getElementById('checkout-date');

      function calculateTotalPrice() {
        const pricePerNightValue = parseFloat(pricePerNight.textContent.substring(1));
        const numNights = parseInt(numNightsInput.value);
        const cleaningFee = 99;
        const totalPriceValue = (pricePerNightValue * numNights) + cleaningFee;
        totalPrice.textContent = '£' + totalPriceValue.toFixed(2);
      }

      function calculateNumNights() {
        const checkinDate = new Date(checkinDateInput.value);
        const checkoutDate = new Date(checkoutDateInput.value);
        const timeDiff = checkoutDate.getTime() - checkinDate.getTime();
        const numNights = Math.ceil(timeDiff / (1000 * 3600 * 24));
        numNightsInput.value = numNights;
        calculateTotalPrice();
      }

      checkinDateInput.addEventListener('change', calculateNumNights);
      checkoutDateInput.addEventListener('change', calculateNumNights);
      numNightsInput.addEventListener('change', calculateTotalPrice);

      calculateTotalPrice();
    });

    let map;

    function initMap() {
      map = new google.maps.Map(document.getElementById("map"), {
        center: { lat: 0, lng: 0 },
        zoom: 12,
      });

      updateMapWithAddress('<%= @flat.address %>');
    }

    function updateMapWithAddress(address) {
  const geocoder = new google.maps.Geocoder();

  geocoder.geocode({ address: address }, function (results, status) {
    if (status === google.maps.GeocoderStatus.OK) {
      const location = results[0].geometry.location;
      const lat = location.lat();
      const lng = location.lng();

      map.setCenter({ lat: lat, lng: lng });

      const markerIcon = {
        url: 'data:image/png;base64,', // Replace <your_base64_encoded_image_data> with your base64-encoded image data
        scaledSize: new google.maps.Size(50, 50),
      };

      const marker = new google.maps.Marker({ position: { lat: lat, lng: lng }, map: map, icon: markerIcon });
    } else {
      console.error('Geocode was not successful for the following reason: ' + status);
    }
  });
}
</script>

</body>
</html>
